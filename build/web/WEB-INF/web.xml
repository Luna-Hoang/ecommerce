<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>ChangePro</servlet-name>
        <servlet-class>controller.web.ChangePro</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogResController</servlet-name>
        <servlet-class>controller.web.LogResController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ControllerCart</servlet-name>
        <servlet-class>controller.web.ControllerCart</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminController</servlet-name>
        <servlet-class>controller.admin.AdminController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Comment</servlet-name>
        <servlet-class>controller.web.CommentAjax</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CartAddController</servlet-name>
        <servlet-class>controller.web.CartAddController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>vnpayRefund</servlet-name>
        <servlet-class>controller.web.vnpayRefund</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ChangePro</servlet-name>
        <url-pattern>/ChangePro</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogResController</servlet-name>
        <url-pattern>/LogResController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ControllerCart</servlet-name>
        <url-pattern>/ControllerCart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminController</servlet-name>
        <url-pattern>/AdminController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Comment</servlet-name>
        <url-pattern>/Comment</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CartAddController</servlet-name>
        <url-pattern>/CartAddController</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <error-page>
        <error-code>404</error-code>
        <location>/pages-error-404.jsp</location>
    </error-page>
</web-app>
